<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>XiansAi.Lib</id>
        <version>1.2.5</version>
        <authors>99x</authors>
        <description>This is a library for the Xians.Ai flow projects.</description>
        <projectUrl>https://github.com/XiansAiPlatform/XiansAi.Lib</projectUrl>
        <copyright>Copyright (c) 99x</copyright> 
        <PackageId>XiansAi.Lib</PackageId> 
        <Description> 
        This is a library for the Xians.Ai flow projects. 
        This LIbrary is used by the Flows. It contains the base classes, 
        interfaces and other utilities for creating and running Flows.
        </Description> 
        <PackageProjectUrl>https://github.com/XiansAiPlatform/XiansAi.Lib</PackageProjectUrl> 
        <RepositoryUrl>https://github.com/XiansAiPlatform/XiansAi.Lib</RepositoryUrl> 
        <PackageTags>Xians AI</PackageTags> 
        <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance> 
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild> 
        <dependencies>
            <group targetFramework="net9.0">
                <dependency id="Microsoft.Extensions.Hosting" version="9.0.0" />
                <dependency id="MongoDB.Driver" version="3.1.0" />
                <dependency id="OpenAI" version="2.1.0" />
                <dependency id="Temporalio" version="1.3.1" />
                <dependency id="Microsoft.Extensions.Configuration.Json" version="9.0.0" />
                <dependency id="Microsoft.Extensions.Options" version="9.0.0" />
                <dependency id="Microsoft.Extensions.Options.ConfigurationExtensions" version="9.0.0" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="bin\Release\net9.0\*.dll" exclude="**/.env" target="lib\net9.0" />
    </files>
</package>